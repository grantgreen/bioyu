            <Answer number="2" text="Den er formel, ikke-profit-orienteret, offentlig"/>
            <Answer number="3" text="Den er uformel, profit-orienteret, ikke offentlig"/>
            <Answer number="4" text="Den er formel, profit-orienteret, offentlig"/>
        </Question>
