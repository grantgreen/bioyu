		var t=$(this)
		t.attr('rel',t.attr('title')).attr('title','')
		 .cluetip(function(){ return LectioJSUtils.TooltipHtmlToHtml(t.attr('rel')); }, {});
		if(i++ > 200) return false;
	});
});

LectioPageOps.InitScriptErrorHandling(true, false);
LectioPageOps.PrepareAutoFocus();
LectioPageOps.PrepareMaintainScrollPosition();
LectioContextMenu.Initialize(true);

$(function() {
	LectioSendAreaTo.Initialize('s_m_Content_Content_holduvb_printareaUVBbuttonprnt', 'printareaUVB', true, '');

});

