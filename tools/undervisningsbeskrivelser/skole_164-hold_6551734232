<script type="text/javascript">
//<![CDATA[
RegContextCard("/lectio/164", "studieplan/hold_undervisningsbeskrivelse.aspx?holdelementid=6551734232");
if (window.SessionHelper) {
	SessionHelper.Initialize(false, '', null, null);
}

$(function(){ 
	$.cluetip.defaults.hoverIntent.interval = 100;
	$.cluetip.defaults.width = 310;
	$.cluetip.defaults.showTitle = false;
	$.cluetip.defaults.waitImage = false;

	i = 0;
	$('[title]').not('option').each(function(){
		var t=$(this)
		t.attr('rel',t.attr('title')).attr('title','')
		 .cluetip(function(){ return LectioJSUtils.TooltipHtmlToHtml(t.attr('rel')); }, {});
		if(i++ > 200) return false;
	});
});

LectioPageOps.InitScriptErrorHandling(true, false);
LectioPageOps.PrepareAutoFocus();
LectioPageOps.PrepareMaintainScrollPosition();
LectioContextMenu.Initialize(true);

$(function() {
	LectioSendAreaTo.Initialize('s_m_Content_Content_holduvb_printareaUVBbuttonprnt', 'printareaUVB', true, '');

});

//]]>
</script>
</form>
	</div>



</body>
</html>
