	$.cluetip.defaults.hoverIntent.interval = 100;
	$.cluetip.defaults.width = 310;
	$.cluetip.defaults.showTitle = false;
	$.cluetip.defaults.waitImage = false;

	i = 0;
	$('[title]').not('option').each(function(){
		var t=$(this)
		t.attr('rel',t.attr('title')).attr('title','')
